            rdfs:domain <http://www.semanticweb.org/tareq/ontologies/2023/3/untitled-ontology-12/Transaction> ;
            rdfs:range :Payment ;
            rdfs:comment "An object property that links an individual or instance of an e-commerce transaction to the payment details associated with the transaction."@en ;
            rdfs:label "hasPayment"@en .


